{
  "version": 2,
  "builds": [
    {
      "src": "functions/src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["functions/src/**", "functions/src/serviceAccountKey.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "functions/src/index.ts"
    }
  ]
}
